<script>
document.getElementById('haarlem').onclick = function() {
  document.getElementById('adres').innerText = 'adresvoorbeeld1' + '\n' +
  '1234ah Uithoorn' + '\n' + '\n';
}
document.getElementById('uithoorn').onclick = function() {
  document.getElementById('adres').innerText = 'adresvoorbeeld2' + '\n' +  
  '1234ah Haarlem' + '\n' + '\n';
}
</script>
